
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Places {
  id     Int  @id @default(autoincrement())
  name String
  placeId  Int 
  prefecture Prefectures? @relation(fields: [placeId], references: [id])
}

model Prefectures {
  id     Int  @id @default(autoincrement())
  name  String
  places  Places[]
}
