
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Prefecture {
  id     Int  @id @default(autoincrement())
  name  String
  places Place[]
}


model Place {
  id     Int  @id @default(autoincrement())
  name String
  prefecture   Prefecture @relation(fields: [prefectureId], references: [id])
  prefectureId Int  
}

